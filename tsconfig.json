{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./assets/js/dist"
  },
  "include": ["assets/js/src/**/*"],
  "exclude": ["node_modules"]
} 